package exo3;

import java.util.Arrays;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<String> prenoms=Arrays.asList("Ludovic","Laurent", "Fabrice","Mathieu", "Loïc", "Olivier", "Betty", "Benjamin","Simon", "Nicolas");
		prenoms.stream().forEach(p->System.out.println("  "+p));

	}

}
