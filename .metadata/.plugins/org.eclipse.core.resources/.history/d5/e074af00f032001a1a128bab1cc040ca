package exo3;

import java.nio.file.Files;
import java.nio.file.Paths;
import java.util.Arrays;

/**
 * Compter les occurences des mots de Sherlock.txt en ottant au préalable les mots du flux json stop-words-en.json
 * 
 *
 */
public class Etape2 {

	public static void main(String[] args) {
		Files.lines(Paths.get("C:\\Users\\Public\\sherlock.txt")).flatMap(s->Arrays.stream(s.split(" ")));
		

	}

}
